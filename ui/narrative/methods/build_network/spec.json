{
  "ver" : "",
  "authors" : ["tgu2"],
  "contact" : "http://kbase.us/contact-us/",
  "visible" : true,
  "categories" : ["upload"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "corr_matrix_ref",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseExperiments.CorrelationMatrix"]
      }
    },
    {
      "id": "coefficient_threshold",
      "optional":false,
      "advanced":true,
      "allow_multiple":false,
      "default_values":[],
      "field_type" : "text",
      "text_options": {
          "validate_as": "float"
      }
    },
    {
      "id" : "network_obj_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
          "valid_ws_types" : ["KBaseExperiments.Network"],
          "is_output_name" : true
        }
    }
  ],
  "parameter-groups": [
    {
      "id": "filter_on_threshold",
      "parameters": [
        "coefficient_threshold"
      ],
      "optional": true,
      "allow_multiple": false,
      "with_border": false
    }
  ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "GenericsAPI",
      "method" : "build_network",
      "input_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace_name"
        },
        {
          "input_parameter" : "corr_matrix_ref",
          "target_property" : "corr_matrix_ref",
          "target_type_transform": "resolved-ref"
        },
        {
          "input_parameter" : "filter_on_threshold",
          "target_property" : "filter_on_threshold"
        },
        {
          "input_parameter" : "network_obj_name",
          "target_property" : "network_obj_name"
        }
      ],
      "output_mapping" : [
        {
          "narrative_system_variable": "workspace",
          "target_property": "wsName"
        },
        {
            "service_method_output_path": [0, "network_obj_ref"],
            "target_property": "network_obj_ref"
        },
        {
            "service_method_output_path": [0, "report_name"],
            "target_property": "report_name"
        },
        {
            "service_method_output_path": [0, "report_ref"],
            "target_property": "report_ref"
        },
        {
            "constant_value": "16",
            "target_property": "report_window_line_height"
        }
      ]
    }
  },
  "job_id_output_field": "docker"
}
