      "text_options" : {}
    },
    {
      "id" : "matrix_name",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseMatrices"],
        "is_output_name" : true
      }
    },
    {
      "id" : "genome_ref",
      "optional" : true,
      "advanced" : true,
      "allow_multiple" : false,
      "default_values" : [ "" ],
      "field_type" : "text",
      "text_options" : {
        "valid_ws_types" : ["KBaseGenomes.Genome"]
      }
    },
    {
      "id" : "col_attributemapping_ref",
      "optional" : true,
      "advanced" : true,
      "allow_multiple" : false,
