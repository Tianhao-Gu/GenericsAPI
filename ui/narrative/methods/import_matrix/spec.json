  "visible" : true,
  "categories" : ["upload"],
  "widgets": {
    "input": "kbaseNarrativeMethodInput",
    "output" : "no-display"
  },
  "parameters" : [
    {
      "id" : "obj_type",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : ["ExpressionMatrix"],
      "field_type" : "dropdown",
      "dropdown_options": 
      {
        "options": 
        [
          {
              "display": "ExpressionMatrix",
              "value": "ExpressionMatrix"
          },
          {
              "display": "FitnessMatrix",
              "value": "FitnessMatrix"
          },
          {
              "display": "DifferentialExpressionMatrix",
              "value": "DifferentialExpressionMatrix"
          }
        ]
      }
    },
    {
      "id" : "input_staging_file_path",
      "optional" : false,
      "advanced" : false,
      "allow_multiple" : false,
      "default_values" : [ "" ],
